@namespace WebAdmin.Components

<MudTabs Elevation="2"  ApplyEffectsToContainer="true" PanelClass="pa-6">
    <MudTabPanel Text="Thông tin" >
        <UserInfo Id="@Id"/>        
    </MudTabPanel>
    <MudTabPanel Text="Lịch sử thuê">
        <OrderList UserId="@Id"/>
    </MudTabPanel>
    <MudTabPanel Text="Lịch sử giao dịch">
       <TransactionList UserId="@Id" />
    </MudTabPanel>
    <MudTabPanel Text="Lịch sử báo cáo">
       <UserReportList UserId="@Id" />
    </MudTabPanel>
</MudTabs>
